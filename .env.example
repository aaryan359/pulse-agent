# Monitoring Agent Configuration
# Copy this file to .env and fill in your values

# Required: Your API key from the dashboard
AGENT_API_KEY=your_api_key_here

# Backend API endpoint
AGENT_BACKEND_URL=https://api.yourapp.com

# Server identifier (defaults to hostname if not set)
AGENT_SERVER_ID=my-server-01

# Collection interval in seconds (default: 10)
AGENT_INTERVAL=10

# Environment tag (production/staging/development)
AGENT_ENV=production

# Log level (debug/info/warn/error)
LOG_LEVEL=info